{
  "name": "insight-overlay",
  "nodecg": {
    "compatibleRange": "~0.8.0",
    "bundleDependencies": {
    },
    "assetCategories": [
      {
        "name": "teamlogos",
        "title": "Team Scoreboard Logos",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "png"
        ]
      }
    ],
    "dashboardPanels": [
      {
        "name": "scoreboard-control",
        "title": "Scoreboard",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "scoreboard-control.html"
      }
    ],
    "graphics": [
      {
        "file": "scoreboard.html",
        "width": 1280,
        "height": 720,
        "singleInstance": false
      }
    ],
    "soundCues": [
      {
        "name": "scoreboard-in",
        "assignable": false,
        "defaultFile": "sound/scoreboard-in.ogg"
      },
      {
        "name": "scoreboard-out",
        "assignable": false,
        "defaultFile": "sound/scoreboard-out.ogg"
      }
    ]
  }
}
